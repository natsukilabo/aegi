<%- include('./common/twcard') %>
<style>
.vui.cast.list{
    display:flex;
    justify-content:flex-start;
    padding:0px;
    margin:0px;
}
.vui.cast.list .cast{
    position:relative;
    width:50%;
    padding:0px;
    margin:0px;
}
.vui.cast.list .cast img{
    display:block;
    width:100%;
    padding:0px;
    margin:0px;
    vertical-align:bottom;
}
.vui.cast.list .cast .name{
    position:absolute;
    bottom:0;
    left:0;
    right:0;
    background-color:rgba(0,0,0,0.7);
    color:#fff;
    width:100%;
    line-height:2.5rem;
    text-align:center;
    padding:0px;
    margin:0px;
}
</style>
</head>
<body>
<%- include('./common/header',{'path':'livelist','title':'配信中のキャスト一覧'}) %>
<div class="vui main">
<div class="vui cast list">
<% if(casts !== undefined){casts.forEach(function(cast){ %>
<a class="cast" href="<%= process.env.app_url %>live/@<%= cast.username %>">
<img src="<%= cast.profile_icon %>">
<span class="name">
<%= cast.name %>
</span>
</a>
<% })} %>
</div>
</div>
</div>
<%- include('./common/footer',{'path':'livelist'}) %>
</body>
